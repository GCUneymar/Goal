<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sunnyweather.android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.magicPocket.android.SunnyWeatherApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.weather.WeatherActivity" />
        <activity android:name="com.magicPocket.android.HomePage" />
        <activity android:name=".User.Modify" />
        <activity android:name=".User.Register" />
        <activity android:name="com.magicPocket.android.CalActivity"/>
        <activity android:name="com.magicPocket.android.joke.JokeActivity" />
        <activity android:name="com.magicPocket.android.joke.JokeDetails" />
        <activity android:name="com.magicPocket.android.Main2" />
        <activity android:name="com.magicPocket.android.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>